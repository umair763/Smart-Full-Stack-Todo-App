{
    "version": 2,
    "name": "smart-todo-backend",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"]
        }
    ],
    "functions": {
        "index.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}
