{
   "version": 2,
   "builds": [
      {
         "src": "package.json",
         "use": "@vercel/static-build",
         "config": {
            "distDir": "dist"
         }
      }
   ],
   "routes": [
      {
         "src": "/assets/(.*)",
         "dest": "/assets/$1",
         "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/favicon.ico",
         "dest": "/favicon.ico",
         "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/manifest.json",
         "dest": "/manifest.json",
         "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/robots.txt",
         "dest": "/robots.txt",
         "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/api/(.*)",
         "dest": "https://smart-todo-task-management-backend.vercel.app/api/$1"
      },
      {
         "src": "/socket.io/(.*)",
         "dest": "https://smart-todo-task-management-backend.vercel.app/socket.io/$1"
      },
      {
         "src": "/(.*)",
         "dest": "/index.html",
         "headers": {
            "Cache-Control": "public, max-age=0, must-revalidate"
         }
      }
   ]
}
