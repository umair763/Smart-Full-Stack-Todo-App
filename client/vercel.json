{
   "version": 2,
   "framework": "vite",
   "buildCommand": "npm run build",
   "outputDirectory": "dist",
   "rewrites": [
      {
         "source": "/(.*)",
         "destination": "/index.html"
      }
   ],
   "routes": [
      {
         "src": "/assets/(.*)",
         "dest": "/assets/$1",
         "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|gif|woff|woff2|ttf|eot))",
         "dest": "/$1",
         "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
         }
      },
      {
         "src": "/api/(.*)",
         "dest": "https://smart-todo-task-management-backend.vercel.app/api/$1",
         "headers": {
            "Cache-Control": "no-cache, no-store, must-revalidate"
         }
      },
      {
         "src": "/socket.io/(.*)",
         "dest": "https://smart-todo-task-management-backend.vercel.app/socket.io/$1",
         "headers": {
            "Cache-Control": "no-cache, no-store, must-revalidate"
         }
      },
      {
         "src": "/(.*)",
         "dest": "/index.html"
      }
   ]
}
