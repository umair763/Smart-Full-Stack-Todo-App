{
	"version": 2,
	"buildCommand": "cd client && npm install && npm run build",
	"outputDirectory": "client/dist",
	"builds": [
		{
			"src": "server/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/server/index.js"
		},
		{
			"src": "/socket.io/(.*)",
			"dest": "/server/index.js"
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"dest": "/client/dist/index.html"
		}
	]
}
