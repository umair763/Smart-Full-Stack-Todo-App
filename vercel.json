{
	"version": 2,
	"builds": [
		{
			"src": "server/index.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["server/**", "dist/**"]
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/server/index.js"
		},
		{
			"src": "/socket.io/(.*)",
			"dest": "/server/index.js"
		},
		{
			"src": "/(.*)",
			"dest": "/server/index.js"
		}
	],
	"env": {
		"NODE_ENV": "production"
	}
}
